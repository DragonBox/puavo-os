[Desktop Entry]
Name=Workspace overview
Exec=dbus-send --session --type=method_call --dest=org.gnome.Shell /org/gnome/Shell org.gnome.Shell.Eval 'string:if (Main.overview.visible) Main.overview.hide(); else Main.overview.show();'
Type=Application
Terminal=false
Icon=preferences-system-windows
NoDisplay=true
